{
    "name": "kusabi/dot",
    "description": "A dot notation library for PHP",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable" : true,
    "authors": [
        {
            "name": "Christian Harvey",
            "email": "hikusabi@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-json": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~2.15",
        "phpbench/phpbench": "^0.13.0",
        "phpunit/phpunit": "^5.7",
        "symfony/var-dumper": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Kusabi\\Dot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kusabi\\Dot\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "5.6"
        },
        "sort-packages": true
    }
}
